import java.util.Scanner;

public class Solution {
    
    public static void main(String[] args) {
        // Write your code here
        
        Scanner s=new Scanner(System.in);
		String st=s.next();
		char c=st.charAt(0);
		if (c>=65 && c<=90){
			System.out.print("1");
		}
		
		else if (c>=97 && c<=122){
			System.out.print("0");
		}
		
		else {
			System.out.print("-1");
		}

    }
}
