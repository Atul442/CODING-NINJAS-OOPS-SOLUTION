import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		// Write your code here
        int i=1;
		Scanner s= new Scanner(System.in);
		int value=s.nextInt();
		int len=1;
		while (len<=value) {
			int ans=(3*i)+2;
			if (ans%4!=0) {
				System.out.print(ans+" ");
				len+=1;
			}
			i+=1;
		}

	}
}
